{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "no-underscore-dangle": 0,
    "react/react-in-jsx-scope": 0,
    "no-nested-ternary": 0,
    "camelcase": 0,
    "consistent-return": 0,
    "no-console": 0,
    "operator-linebreak": 0,
    "implicit-arrow-linebreak": 0,
    "object-curly-newline": 0,
    "indent": 0
  }
}
